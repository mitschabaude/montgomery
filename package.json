{
  "name": "montgomery",
  "version": "0.3.1",
  "description": "Fast implementation of multi-scalar multiplication in WebAssembly",
  "main": "./build/src/index.js",
  "type": "module",
  "types": "./build/src/index.d.ts",
  "exports": {
    "types": "./build/src/index.d.ts",
    "default": "./build/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitschabaude/montgomery"
  },
  "files": [
    "src",
    "build",
    "README.md",
    "NOTICE"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc && ./run scripts/test-pasta.ts",
    "start": "node --loader=ts-node/esm scripts/test-bls12377-msm.ts 16",
    "start-web": "npx chrode ./scripts/run.web.ts --no-headless",
    "evaluate": "node --loader=ts-node/esm scripts/evaluate-msm-pasta.ts",
    "test": "node --loader=ts-node/esm  scripts/test-pasta.ts",
    "test-msm": "node --loader=ts-node/esm scripts/test-pasta-msm.ts",
    "benchmark": "node --loader=ts-node/esm scripts/multiply-benchmark.ts",
    "benchmark-377": "node --loader=ts-node/esm scripts/multiply-benchmark-377.ts",
    "benchmark-381": "node --loader=ts-node/esm scripts/multiply-benchmark-381.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rm -rf build"
  },
  "keywords": [],
  "author": "Gregor Mitscha-Baude",
  "license": "Apache-2.0",
  "dependencies": {
    "fast-base64": "^0.1.8",
    "wasmati": "^0.2.0"
  },
  "devDependencies": {
    "chrode": "^0.4.0",
    "eslint": "^8.20.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "wabt": "^1.0.29"
  }
}
